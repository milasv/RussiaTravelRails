<%= form_for @region do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true, :required => true %>
  <%= f.label :about %>
  <%= f.text_field :state %>
  <%= f.label :image %>
  <%= f.url_field :image %>
  <%= f.label :capital %>
  <%= f.text_field :capital %>
  <%= f.submit %>
<% end %>
