<%= form_for @season do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true, :required => true %>
  <%= f.label :about %>
  <%= f.text_field :about %>
  <%= f.label :image %>
  <%= f.url_field :image %>
  <%= f.label :time %>
  <%= f.text_field :time %>
  <%= f.submit %>
<% end %>
